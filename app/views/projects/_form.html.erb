  <%= form_for @project do |f| %>
    <% if @project.errors.any? %>
      <div id="errors">
        <h2><%= pluralize(@project.errors.count, "error")  %> preventing this error from posting:</h2>

        <ul>
          <% @project.errors.full_messages.each do |error| %>
            <li><%= error %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <br>
    <%= f.label :description %><br>
    <%= f.text_area :description %><br>
    <br>
    <%= f.label :github_url %><br>
    <%= f.text_field :github_url %><br>
    <br>
    <%= f.label :project_url %><br>
    <%= f.text_field :project_url %><br>
    <br>
    <%= f.submit %>
  <% end %>
